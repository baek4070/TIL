	"korea html" :{
		"prefix": "korea-html",
		"body": [
			"<!doctype html>",
			"<html lang=\"ko\">",
			"<head>",
			"\t<meta charset=\"utf-8\"/>",
			"\t<title>$1</title>",
			"</head>",
			"<body>",
			"</body>",
			"</html>"
		],
		"description": "한국어 html 자동완성 추가"
	}
